<?xml version="1.0"?>
<extension key="inmobile_sms" type="module">
  <file>inmobile_sms</file>
  <name>inmobile_sms</name>
  <description>Interface to Inmobile SMS gateway</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>FIXME</author>
    <email>FIXME</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-09-03</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.59</ver>
  </compatibility>
  <comments>This module interfaces to Inmobile API </comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/InmobileSms</namespace>
    <format>23.02.1</format>
    <angularModule>crmInmobileSms</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <upgrader>CRM_InmobileSms_Upgrader</upgrader>
</extension>
